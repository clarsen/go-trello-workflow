{
  "rulesDirectory": [],
  "extends": ["tslint-config-airbnb", "tslint:latest", "tslint-react"],
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "import-name": false,
    "no-boolean-literal-compare": false,
    "no-unexpected-multiline": true,
    "no-extra-semi": true,
    "semicolon": [true, "never"],
    "no-console": false,
    "quotemark": [true, "single"]
  }

}